{
  "name": "Harvestor",
  "description": "Extracts NFT Wallets from Twitter",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.js",
    "default_icon": {
        "48": "./images/logo_48.png",
        "128": "./images/logo_128.png"
      }
  },
  "background": {
    "service_worker": "extract.js"
  },
  "icons": {
    "48": "./images/logo_48.png",
    "128": "./images/logo_128.png"
  }
}
